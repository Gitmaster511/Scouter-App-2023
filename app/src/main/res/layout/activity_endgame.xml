<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <!-- Top Bar  -->

    <!-- CheckBoxes -->

    <!-- Checkboxes -->


    <!-- Bottom Navigation -->

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="115dp"
        android:background="@drawable/top_rect"
        android:bottomLeftRadius="40dp"
        android:bottomRightRadius="40dp"
        android:topLeftRadius="40dp"
        android:topRightRadius="40dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/textView"
        android:layout_width="343dp"
        android:layout_height="94dp"
        android:layout_marginStart="189dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="129dp"
        android:fontFamily="@font/avenirbold"
        android:text="End Game"
        android:textColor="@color/white"
        android:textSize="70sp"
        app:fontFamily="@font/avenirbold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckBox
        android:id="@id/attempted"
        android:layout_width="260dp"
        android:layout_height="85dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="127dp"
        android:background="@drawable/smaller_rect_field"
        android:buttonTint="#ffffff"
        android:fontFamily="@font/avenir"
        android:text="Attempted"
        android:textColor="@color/white"
        android:textSize="35dp"
        app:fontFamily="@font/avenir"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckBox
        android:id="@id/docked"
        android:layout_width="260dp"
        android:layout_height="85dp"
        android:layout_marginTop="127dp"
        android:layout_marginEnd="26dp"
        android:background="@drawable/smaller_rect_field"
        android:buttonTint="#ffffff"
        android:fontFamily="@font/avenir"
        android:text="Docked"
        android:textColor="@color/white"
        android:textSize="35dp"
        app:fontFamily="@font/avenir"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckBox
        android:id="@id/engaged"
        android:layout_width="260dp"
        android:layout_height="85dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="249dp"
        android:background="@drawable/smaller_rect_field"
        android:buttonTint="#ffffff"
        android:fontFamily="@font/avenir"
        android:text="Engaged"
        android:textColor="@color/white"
        android:textSize="35dp"
        app:fontFamily="@font/avenir"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckBox
        android:id="@+id/gave_assistance"
        android:layout_width="457dp"
        android:layout_height="85dp"
        android:layout_marginTop="359dp"
        android:background="@drawable/smaller_rect_field"
        android:buttonTint="#ffffff"
        android:fontFamily="@font/avenir"
        android:text="Gave Assistance"
        android:textColor="@color/white"
        android:textSize="45dp"
        app:fontFamily="@font/avenir"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckBox
        android:id="@+id/soloclimb"
        android:layout_width="260dp"
        android:layout_height="85dp"
        android:layout_marginTop="249dp"
        android:layout_marginEnd="26dp"
        android:background="@drawable/smaller_rect_field"
        android:buttonTint="#ffffff"
        android:fontFamily="@font/avenir"
        android:text="Solo Climb"
        android:textColor="@color/white"
        android:textSize="35dp"
        app:fontFamily="@font/avenir"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <CheckBox
        android:id="@+id/parked"
        android:layout_width="187dp"
        android:layout_height="66dp"
        android:layout_marginTop="583dp"
        android:background="@drawable/smaller_rect_field"
        android:buttonTint="#ffffff"
        android:fontFamily="@font/avenir"
        android:text="Parked"
        android:textColor="@color/white"
        android:textSize="35dp"
        app:fontFamily="@font/avenir"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckBox
        android:id="@+id/recieved_assistance"
        android:layout_width="457dp"
        android:layout_height="85dp"
        android:layout_marginTop="469dp"
        android:background="@drawable/smaller_rect_field"
        android:buttonTint="#ffffff"
        android:fontFamily="@font/avenir"
        android:text="Received Assistance"
        android:textColor="@color/white"
        android:textSize="45dp"
        app:fontFamily="@font/avenir"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view3"
        android:layout_width="match_parent"
        android:layout_height="94dp"
        android:background="@drawable/bottom_rect"
        android:bottomLeftRadius="40dp"
        android:bottomRightRadius="40dp"
        android:topLeftRadius="40dp"
        android:topRightRadius="40dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="166dp"
        android:layout_height="75dp"
        android:layout_marginStart="200dp"
        android:layout_marginEnd="200dp"
        android:layout_marginBottom="10dp"
        android:fontFamily="@font/avenir"
        android:text="Page 4"
        android:textColor="@color/white"
        android:textSize="50sp"
        app:fontFamily="@font/avenir"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="-91dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="-3dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="-1dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="654dp" />

    <Button
        android:id="@+id/Forward_page_4"
        android:layout_width="55dp"
        android:layout_height="65dp"
        android:layout_marginEnd="140dp"
        android:layout_marginBottom="12dp"
        android:background="@drawable/triangleforward"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/Backward_page_4"
        android:layout_width="55dp"
        android:layout_height="65dp"
        android:layout_marginStart="132dp"
        android:layout_marginBottom="14dp"
        android:background="@drawable/triangleforward"
        android:rotation="180"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <SeekBar
        android:id="@+id/seekBar"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="match_parent"
        android:layout_height="62dp"
        android:layout_marginTop="748dp"
        android:max="60"
        android:progress="0"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="695dp" />


</androidx.constraintlayout.widget.ConstraintLayout>

