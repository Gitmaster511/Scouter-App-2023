<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteX="134dp"
    tools:layout_editor_absoluteY="1dp">

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="137dp"
        android:background="@drawable/top_rect"
        tools:layout_editor_absoluteX="-4dp"
        tools:layout_editor_absoluteY="-4dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="End Notes"
        android:textColor="#FFFFFF"
        android:textSize="75sp"
        tools:layout_editor_absoluteX="92dp"
        tools:layout_editor_absoluteY="16dp" />

    <View
        android:id="@+id/view4"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:background="@drawable/bottom_rect"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="822dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Page 5"
        android:textColor="#FFFFFF"
        android:textSize="50sp"
        tools:layout_editor_absoluteX="223dp"
        tools:layout_editor_absoluteY="860dp" />

    <View
        android:id="@+id/view5"
        android:layout_width="46dp"
        android:layout_height="49dp"
        android:background="@drawable/triangle"
        tools:layout_editor_absoluteX="391dp"
        tools:layout_editor_absoluteY="870dp" />

    <View
        android:id="@+id/view6"
        android:layout_width="46dp"
        android:layout_height="49dp"
        android:background="@drawable/triangle"
        android:rotation="180"
        tools:layout_editor_absoluteX="163dp"
        tools:layout_editor_absoluteY="870dp" />


    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="match_parent"
        android:layout_height="109dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Additional Notes: "
        android:textSize="30dp"
        android:hint="Ex:the robot stopped"
        android:textColorHint="#0000ff"
        android:textColor="#0000FF"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="613dp" />

    <SeekBar
        android:id="@+id/seekBar2"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="546dp"
        android:layout_height="135dp"
        android:max="4"
        android:progress="0"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="173dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Aggression Of The Robot "
        android:textColor="#0000FF"
        android:textSize="35sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="150dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1"
        android:textColor="#0000ff"
        android:textSize="35sp"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="253dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="5"
        android:textColor="#0000ff"
        android:textSize="35sp"
        tools:layout_editor_absoluteX="542dp"
        tools:layout_editor_absoluteY="253dp" />



    <CheckBox
        android:id="@+id/yescheckBox"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text=" Yes"
        android:textColor="#FFFFFF"
        android:textSize="45sp"
        android:background="@drawable/smaller_rect_field"
        tools:layout_editor_absoluteX="272dp"
        tools:layout_editor_absoluteY="742dp" />

    <CheckBox
        android:id="@+id/nocheckBox3"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:background="@drawable/smaller_rect_field"
        android:text=" No"
        android:textColor="#FFFFFF"
        android:textSize="45sp"
        tools:layout_editor_absoluteX="434dp"
        tools:layout_editor_absoluteY="742dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Win?"
        android:textColor="#0000ff"
        android:textSize="45sp"
        tools:layout_editor_absoluteX="65dp"
        tools:layout_editor_absoluteY="749dp" />


</androidx.constraintlayout.widget.ConstraintLayout>