<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteX="134dp"
    tools:layout_editor_absoluteY="1dp">

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="115dp"
        android:background="@drawable/top_rect"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="-4dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="128dp"
        android:layout_marginTop="24dp"
        android:text="End Notes"
        android:textColor="#FFFFFF"
        android:fontFamily="@font/avenirbold"
        app:fontFamily="@font/avenirbold"
        android:textSize="75sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view4"
        android:layout_width="610dp"
        android:layout_height="94dp"
        android:layout_marginBottom="0dp"
        android:background="@drawable/bottom_rect"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="94dp"
        android:text="Page 5"
        android:textColor="#FFFFFF"
        android:textSize="50sp"
        android:fontFamily="@font/avenir"
        app:fontFamily="@font/avenir"
        tools:layout_editor_absoluteX="223dp"
        tools:layout_editor_absoluteY="878dp" />


    <Button
        android:id="@+id/Forward_page_4"
        android:layout_width="77dp"
        android:layout_height="76dp"
        android:layout_marginEnd="132dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/triangleforward"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/Backward_page_4"
        android:layout_width="77dp"
        android:layout_height="76dp"
        android:layout_marginStart="144dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/triangleforward"
        android:rotation="180"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="match_parent"
        android:layout_height="109dp"
        android:layout_marginTop="613dp"
        android:ems="10"
        android:hint="Ex:the robot stopped"
        android:inputType="textPersonName"
        android:text="                     Additional Notes: "
        android:textColor="#000000"
        android:fontFamily="@font/avenir"
        app:fontFamily="@font/avenir"
        android:textColorHint="#0000ff"
        android:textSize="30dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <SeekBar
        android:id="@+id/seekBar2"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="589dp"
        android:layout_height="159dp"

        android:layout_marginTop="196dp"
        android:max="4"
        android:progress="0"
        android:progressDrawable="@drawable/progress"

        android:thumb="@drawable/icons8_round_48"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.304"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="150dp"
        android:text="Aggression Of The Robot "
        android:textColor="#000000"
        android:textSize="35sp"
        android:fontFamily="@font/avenir"
        app:fontFamily="@font/avenir"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1"
        android:textColor="#0000ff"
        android:textSize="35sp"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="197dp" />


    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2"
        android:textColor="#C40818"
        android:textSize="35sp"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="197dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1"
        android:textColor="#C40818"
        android:textSize="35sp"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="197dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="4"
        android:textColor="#C40818"
        android:textSize="35sp"
        tools:layout_editor_absoluteX="419dp"
        tools:layout_editor_absoluteY="197dp" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="3"
        android:textColor="#C40818"
        android:textSize="35sp"
        tools:layout_editor_absoluteX="290dp"
        tools:layout_editor_absoluteY="197dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="5"
        android:textColor="#C40818"
        android:textSize="35sp"
        tools:layout_editor_absoluteX="545dp"
        tools:layout_editor_absoluteY="197dp" />

    <CheckBox
        android:id="@+id/yescheckBox"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:background="@drawable/smaller_rect_field"
        android:text=" Yes"
        android:textColor="#FFFFFF"
        android:textSize="45sp"
        tools:layout_editor_absoluteX="241dp"
        tools:layout_editor_absoluteY="749dp" />

    <CheckBox
        android:id="@+id/nocheckBox3"
        android:layout_width="146dp"
        android:layout_height="64dp"
        android:layout_marginTop="748dp"
        android:layout_marginEnd="36dp"
        android:background="@drawable/smaller_rect_field"
        android:text=" No"
        android:textColor="#FFFFFF"
        android:textSize="45sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="65dp"
        android:layout_marginTop="749dp"
        android:text="Win?"
        android:fontFamily="@font/avenir"
        app:fontFamily="@font/avenir"
        android:textColor="#000000"
        android:textSize="45sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>