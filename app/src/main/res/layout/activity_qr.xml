<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteX="134dp"
    tools:layout_editor_absoluteY="1dp">

    <View
        android:layout_width="match_parent"
        android:layout_height="115dp"
        android:background="@drawable/top_rect"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="251dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/avenirbold"
        android:text="QR"
        android:textColor="#FFFFFF"
        android:textSize="75sp"
        app:fontFamily="@font/avenirbold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:layout_width="match_parent"
        android:layout_height="94dp"
        android:layout_marginBottom="0dp"
        android:background="@drawable/bottom_rect"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="222dp"
        android:layout_marginTop="877dp"
        android:layout_marginBottom="25dp"
        android:fontFamily="@font/avenir"
        android:text="Page 6"
        android:textColor="#FFFFFF"
        android:textSize="50sp"
        app:fontFamily="@font/avenir"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/view9"
        android:layout_width="549dp"
        android:layout_height="565dp"
        android:layout_marginStart="67dp"
        android:layout_marginTop="222dp"
        android:layout_marginEnd="68dp"
        android:layout_marginBottom="269dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.373"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.914" />

    <Button
        android:id="@+id/Backward_page_5"

        android:layout_width="55dp"
        android:layout_height="65dp"
        android:layout_marginStart="148dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/triangleforward"
        android:rotation="180"
        app:backgroundTint="@color/white"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button"
        android:layout_width="245dp"
        android:layout_height="98dp"
        android:layout_marginStart="178dp"
        android:layout_marginEnd="178dp"
        android:layout_marginBottom="105dp"
        android:text="Generate"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/resethard"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="484dp"
        android:layout_marginTop="868dp"
        android:layout_marginEnd="27dp"
        android:background="@drawable/restart_icon_32263"
        app:backgroundTint="#EA1919"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <!-- android:background="@drawable/restart_icon_32263" -->


</androidx.constraintlayout.widget.ConstraintLayout>